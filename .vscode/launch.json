{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "FunCards Debug",
            "type": "chrome",
            "request": "launch",
            "url": "http://localhost:1234",
            "breakOnLoad": true,
            "webRoot": "${workspaceFolder}",
            "pathMapping": {
                "__parcel_source_root": "${workspaceFolder}"
            },
            "skipFiles": [
                "${workspaceFolder}/node_modules/**/*.js",
                "<node_internals>/**"
            ]
        }
    ]
}